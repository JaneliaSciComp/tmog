<?xml version="1.0"?>

<transmogrifierConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                     xsi:noNamespaceSchemaLocation="transmogrifier_config.xsd">

    <global minimumVersion="3.0.4" frameSizePercentage="95"/>

    <project name="Fly Light Polarity - preScreen - scope to dm11" default="true">

        <inputFileFilter patternString=".+\.lsm" />

        <!-- NOTES:
             - first capturing group must be equal for second capturing group
               to be used in numeric sort
             - third capturing group only used when first and second are equal
             - '(?: )' identifies a non-capturing group
        -->
        <inputFileSorter sortAlgorithm="Number"
                         patternString="(?:^|.*_)(B)(\d+)(_.*)"/>

        <outputDirectory>
            <path path="[\\dm11.janelia.priv|/groups/flylight]/flylight/polarity/preScreen/" />
            <renameFieldValue fieldDisplayName="Mount Date"/>
            <path path="_"/>
            <renameFieldValue fieldDisplayName="Slide Number"/>
            <path path="/"/>
            <renameFieldValue fieldDisplayName="Magnification"/>
        </outputDirectory>

        <plugins>
            <rowUpdater className="org.janelia.it.ims.tmog.plugin.dataFile.DataResourcePlugin">
                <property name="plugin.service-url" value="http://sage.int.janelia.org/flyboy-ws/project_crosses/withSerialNumber/${Cross Serial Number}" />
                <property name="plugin.test-url" value="http://sage.int.janelia.org/flyboy-ws/project_crosses/withSerialNumber/51418" />
                <property name="plugin.root-xpath" value="projectCross" />
                <property name="Line" value="parent1StockName" />
                <property name="Reporter" value="reporter" />
            </rowUpdater>

            <rowUpdater className="org.janelia.it.ims.tmog.plugin.dataFile.ZeissLsmDataPlugin">
                <property name="plugin-scale" value="0" />
                <property name="Channel 2 Name" value="IlluminationChannel #2 Name" />
                <property name="Channel 2 Power" value="IlluminationChannel #2 Power B/C 1" />
                <property name="Channel 2 Gain" value="DetectionChannel #2 Detector Gain" />
            </rowUpdater>

            <!--
                  The companion file listener (for log files) should be the
                  last defined plug-in to ensure that log files are only
                  copied if the source lsm processing succeeds.
            -->
            <rowListener className="org.janelia.it.ims.tmog.plugin.CompanionFileRowListener" >
                <property name="sourceSuffix" value=".lsm"/>
                <property name="companionSuffix" value="_log.csv"/>
                <property name="deleteAfterCopy" value="true"/>
            </rowListener>

        </plugins>

        <dataFields>

            <text displayName="Line" required="true"
                  suffix="__"/>

            <number displayName="Cross Serial Number" required="true"
                  minimumValue="1"
                  maximumValue="99999"
                  suffix="_">
            </number>

            <date displayName="Mount Date" required="true"
                  datePattern="yyyyMMdd"
                  suffix="_">
                <sourceFileDateDefault pattern=".*_(\d{8})_.*"
                                       fromDatePattern="yyyyMMdd"
                                       toDatePattern="yyyyMMdd"/>
            </date>

            <text displayName="Slide Number" required="true"
                  pattern="[A-Za-z]*\d+"
                  suffix="_">
                <sourceFileDefault pattern=".*_\d{8}_([^_]+)_.*"/>
            </text>

            <text displayName="Slide Location" required="true"
                  pattern="[A-Z]\d\d?"
                  convertToUpperCase="true"
                  suffix="_">
                <sourceFileDefault pattern="^([^_]+)_ZB.*"/>
                <sourceFileSlideLocationDefault pattern="(?:^|.*_)B(\d+)_.*"
                                                numberOfSlideColumns="6"/>
            </text>

            <number displayName="Zeiss Brain" required="true"
                    minimumValue="1"
                    maximumValue="99"
                    padFormat="20"
                    copyable="false"
                    prefix="zb"
                    suffix="_">
                <sourceFileDefault pattern=".*_[Zz][Bb](\d+)_.*"/>
                <sourceFileDefault pattern="(?:^|.*_)B(\d+)_.*"/>
            </number>

            <number displayName="Zeiss Tile" required="true"
                    minimumValue="1"
                    maximumValue="99"
                    padFormat="20"
                    prefix="zt"
                    suffix="_">
                <sourceFileDefault pattern=".*_zt(\d+)_.*"/>
                <sourceFileDefault pattern=".*_T(\d+)_.*"/>
            </number>

            <validValueList displayName="Magnification" required="true"
                            autoComplete="true"
                            suffix="_">
                <sourceFileDefault pattern=".*_(\d\dX)_.*"/>
                <validValue value="20X"/>
                <validValue value="63X"/>
            </validValueList>

            <text displayName="Channel 2 Name" required="true"
                  suffix="-">
            </text>

            <number displayName="Channel 2 Power" required="true"
                    suffix="-">
            </number>

            <number displayName="Channel 2 Gain" required="true"
                    suffix="_">
            </number>

            <text displayName="Reporter"
                  prefix="_"
                  suffix="__">
            </text>

            <text displayName="Suffix" required="true"
                  pattern="R\d+_L\d+"
                  copyable="false">
                <sourceFileDefault pattern=".*_(R\d+_L\d+).*"/>
            </text>

            <separator value="_"/>
            <runTime datePattern="yyyyMMddHHmmssSSS" />
            <separator value=".lsm"/>

        </dataFields>

    </project>

