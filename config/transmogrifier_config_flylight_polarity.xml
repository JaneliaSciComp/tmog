    <project name="Fly Light Polarity - preScreen - fix" taskName="simple-move">

        <inputFileFilter patternString=".+\.lsm" recursiveSearch="true"/>

        <!-- NOTES:
             - first capturing group must be equal for second capturing group
               to be used in numeric sort
             - third capturing group only used when first and second are equal
             - '(?: )' identifies a non-capturing group
        -->
        <inputFileSorter sortAlgorithm="Number"
                         patternString="(?:^|.*_)(zb)(\d+)(_.*)"/>

        <outputDirectory derivedFromSourceFile="true"/>

        <plugins>
            <rowUpdater className="org.janelia.it.ims.tmog.plugin.dataFile.DataResourcePlugin">
                <property name="plugin.service-url" value="http://sage.int.janelia.org/flyboy-ws/project_crosses/withSerialNumber/${Cross Serial Number}" />
                <property name="plugin.test-url" value="http://sage.int.janelia.org/flyboy-ws/project_crosses/withSerialNumber/51418" />
                <property name="plugin.root-xpath" value="projectCross" />
                <property name="Line" value="parent1StockName" />
                <property name="Reporter" value="reporter" />
            </rowUpdater>

            <!--
                  The companion file listener (for log files) should be the
                  last defined plug-in to ensure that log files are only
                  copied if the source lsm processing succeeds.
            -->
            <rowListener className="org.janelia.it.ims.tmog.plugin.CompanionFileRowListener" >
                <property name="sourceSuffix" value=".lsm"/>
                <property name="companionSuffix" value="_log.csv"/>
                <property name="deleteAfterCopy" value="true"/>
            </rowListener>
        </plugins>

        <dataFields>

            <text displayName="Line" required="true"
                  suffix="__">
                <sourceFileDefault pattern="^(.*)__\d{5}_.*"/>
            </text>

            <number displayName="Cross Serial Number" required="true"
                  minimumValue="1"
                  maximumValue="99999"
                  suffix="_">
                <sourceFileDefault pattern="^.*__(\d{5})_\d{8}_.*"/>
            </number>

            <date displayName="Mount Date" required="true"
                  datePattern="yyyyMMdd"
                  suffix="_">
                <sourceFileDateDefault pattern=".*_\d{5}_(\d{8})_.*"
                                       fromDatePattern="yyyyMMdd"
                                       toDatePattern="yyyyMMdd"/>
            </date>

            <text displayName="Slide Number" required="true"
                  pattern="[A-Za-z]*\d+"
                  suffix="_">
                <sourceFileDefault pattern=".*_\d{5}_\d{8}_(\d+)_.*"/>
            </text>

            <text displayName="Slide Location" required="true"
                  pattern="[A-Z]\d\d?"
                  convertToUpperCase="true"
                  suffix="_">
                <sourceFileDefault pattern=".*_\d{5}_\d{8}_\d+_([A-Z]\d\d?).*"/>
            </text>

            <number displayName="Zeiss Brain" required="true"
                    minimumValue="1"
                    maximumValue="99"
                    padFormat="20"
                    copyable="false"
                    prefix="zb"
                    suffix="_">
                <sourceFileDefault pattern=".*_zb(\d+)_.*"/>
            </number>

            <number displayName="Zeiss Tile" required="true"
                    minimumValue="1"
                    maximumValue="99"
                    padFormat="20"
                    prefix="zt"
                    suffix="_">
                <sourceFileDefault pattern=".*_zt(\d+)_.*"/>
            </number>

            <validValueList displayName="Magnification" required="true"
                            autoComplete="true"
                            suffix="_">
                <sourceFileDefault pattern=".*_(\d\dX)_.*"/>
                <validValue value="20X"/>
                <validValue value="63X"/>
            </validValueList>

            <text displayName="Channel 1 or 2 Name" required="true"
                  suffix="-">
                <sourceFileDefault pattern=".*_\d\dX_([^\-_]+).*"/>
            </text>

            <number displayName="Channel 1 or 2 Power" required="true"
                    suffix="-">
                <sourceFileDefault pattern=".*_\d\dX_[^\-_]+\-([^\-_]+).*"/>
            </number>

            <number displayName="Channel 1 or 2 Gain" required="true"
                    suffix="_">
                <sourceFileDefault pattern=".*_\d\dX_[^\-_]+\-[^\-_]+\-([^\-_]+)__.*"/>
            </number>

            <text displayName="Reporter"
                  prefix="_"
                  suffix="__">
                <sourceFileDefault pattern=".*_\d\dX_[^\-_]+\-[^\-_]+\-[^\-_]+__(.*)__.*"/>
            </text>

            <text displayName="Suffix" required="true"
                  pattern="R\d+_L\d+"
                  copyable="false">
                <sourceFileDefault pattern=".*_(R\d+_L\d+).*"/>
            </text>

            <separator value="_"/>
            <text displayName="tmog ID" required="true"
                  pattern="\d{17}">
                <sourceFileDefault pattern=".*_(\d{17}).*"/>
            </text>

            <separator value=".lsm"/>

        </dataFields>

    </project>

