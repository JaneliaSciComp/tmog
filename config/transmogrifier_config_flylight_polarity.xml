        -->
        <inputFileSorter sortAlgorithm="Number"
                         patternString=".*_(\d{8}_\d\d_[A-Z]\d).*_R._L(\d++)_(.*)"/>

        <outputDirectory derivedFromSourceFile="true"/>

        <plugins>
            <rowValidator className="org.janelia.it.ims.tmog.plugin.MoveValidator">
