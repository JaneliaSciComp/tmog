    <project name="Rubin Lab: ChaCRM verified TransformantIDs (GMR_*)" default="true">

        <inputFileSorter sortAlgorithm="LNumber" />

        <outputDirectory basePath="\\dm10.isln.janelia.org\rubindata\confocalStacks"
                         derivedFromEarliestModifiedFile="true"/>

        <plugins>

            <rowValidator className="org.janelia.it.wip.WipDataPlugin">
                <property name="batchNameFieldName" value="Batch Name"/>
                <property name="batchNumberFieldName" value="Batch Number"/>
                <property name="batchColorFieldName" value="Batch Color"/>
            </rowValidator>
            <rowValidator className="org.janelia.it.chacrm.ChacrmEventManager" />

            <rowListener className="org.janelia.it.wip.WipDataPlugin">
                <property name="batchNameFieldName" value="Batch Name"/>
                <property name="batchNumberFieldName" value="Batch Number"/>
                <property name="batchColorFieldName" value="Batch Color"/>
            </rowListener>
            <rowListener className="org.janelia.it.chacrm.ChacrmEventManager" >
                <property name="labImageSharePattern" value="[/\\]++rubindata[/\\]++confocalStacks[/\\]++"/>
            </rowListener>
            <rowListener className="org.janelia.it.ims.tmog.plugin.imagedb.ImageDataPlugin">
                <property name="db.config.key" value="nighthawk"/>
                <property name="family" value="family"/>
                <property name="uas_reporter" value="UAS Reporter"/>
                <property name="line" value="GMR_${Plate}${Well}_${Vector ID}_${Landing Site}"/>
                <property name="created_by" value="Imager"/>
                <property name="renamed_by" value="Renamer"/>
                <property name="class" value="Class"/>
                <property name="ihc_batch" value="Batch Name"/>
                <property name="mounting_protocol" value="Tissue Mounting Protocol"/>
                <property name="tissue_orientation" value="Tissue Orientation"/>
            </rowListener>

            <sessionListener className="org.janelia.it.ims.tmog.plugin.SessionSummaryFileWriter">
                <property name="directory" value="\\dm10.isln.janelia.org\rubindata\tmog\summaries\GMR"/>
            </sessionListener>

        </plugins>

        <dataFields>
            <separator value="GMR_"/>
            <number displayName="Plate" required="true"
                    minimumValue="1" maximumValue="999"/>
            <well displayName="Well" required="true" formFactor="96"/>
            <separator value="_"/>
            <cvTermList displayName="Vector ID" required="true"
                        serviceUrl="http://sage.int.janelia.org/sage-ws/cvs/rubin_crm_vector"
                        displayNamePrefixedForValues="true">
            </cvTermList>
            <separator value="_"/>
