<?xml version="1.0"?>

<transmogrifierConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                     xsi:noNamespaceSchemaLocation="transmogrifier_config.xsd">

    <global minimumVersion="2.6.0" frameSizePercentage="95"/>

    <project name="Rubin Lab: RD Split (RDSP_*)" default="true">

        <inputFileSorter sortAlgorithm="LNumber" />

        <outputDirectory basePath="[\\dm10.isln.janelia.org\rubindata|/groups/rubin/data1/rubinlab]\RD_split\confocalStacks"
                         derivedFromEarliestModifiedFile="true"/>

        <plugins>

            <rowValidator className="org.janelia.it.ims.tmog.plugin.MultipleFieldTextValidator">
                <property name="value" value="${Line 1 (select)}${Line 1 (text)}"/>
                <property name="required" value="true"/>
                <property name="errorMessage" value="Please either select or enter a Line 1 value."/>
            </rowValidator>

            <rowValidator className="org.janelia.it.ims.tmog.plugin.MultipleFieldTextValidator">
                <property name="value" value="${Line 2 (select)}${Line 2 (text)}"/>
                <property name="required" value="true"/>
                <property name="errorMessage" value="Please either select or enter a Line 2 value."/>
            </rowValidator>

            <rowValidator className="org.janelia.it.ims.tmog.plugin.HttpResourceValidator">
                <property name="serviceUrl" value="http://sage.int.janelia.org/sage-ws/lines/rubin/${Line 1 (select)}${Line 1 (text)}.janelia-sage"/>
                <property name="testUrl" value="http://sage.int.janelia.org/sage-ws/lines/rubin/GMR_10A01_AE_01.janelia-sage"/>
                <property name="errorMessage" value="There is no 'rubin' lab line named '${Line 1 (select)}${Line 1 (text)}' in the SAGE database."/>
                <property name="clearCacheDuration" value="6000000"/><!-- 100 minutes -->
                <!--
                The following resources will always be considered valid
                (and will not be checked against the service).
                -->
                <property name="validResources"
                          value="http://sage.int.janelia.org/sage-ws/lines/rubin/c155_Elav.janelia-sage,
                                 http://sage.int.janelia.org/sage-ws/lines/rubin/replace-this-with-another-line-name.janelia-sage,
                                 http://sage.int.janelia.org/sage-ws/lines/rubin/replace-this-with-another-line-name.janelia-sage"/>
            </rowValidator>

            <rowValidator className="org.janelia.it.ims.tmog.plugin.HttpResourceValidator">
                <property name="serviceUrl" value="http://sage.int.janelia.org/sage-ws/lines/rubin/${Line 2 (select)}${Line 2 (text)}.janelia-sage"/>
                <property name="testUrl" value="http://sage.int.janelia.org/sage-ws/lines/rubin/GMR_10A01_AE_01.janelia-sage"/>
                <property name="errorMessage" value="There is no 'rubin' lab line named '${Line 2 (select)}${Line 2 (text)}' in the SAGE database."/>
                <property name="clearCacheDuration" value="6000000"/><!-- 100 minutes -->
                <!--
                The following resources will always be considered valid
                (and will not be checked against the service). 
                -->
                <property name="validResources"
                          value="http://sage.int.janelia.org/sage-ws/lines/rubin/16H08_KD_GS4_P2.janelia-sage,
                                 http://sage.int.janelia.org/sage-ws/lines/rubin/29C07_KD_GS4_P2.janelia-sage,
                                 http://sage.int.janelia.org/sage-ws/lines/rubin/29C07_KD_GS4_P40.janelia-sage,
                                 http://sage.int.janelia.org/sage-ws/lines/rubin/29C07_KD_GS4_P2_P40.janelia-sage,
                                 http://sage.int.janelia.org/sage-ws/lines/rubin/40E09_KD_GS4_P2.janelia-sage,
                                 http://sage.int.janelia.org/sage-ws/lines/rubin/NP0681.janelia-sage,
                                 http://sage.int.janelia.org/sage-ws/lines/rubin/NP1035.janelia-sage,
                                 http://sage.int.janelia.org/sage-ws/lines/rubin/NP3045.janelia-sage,
                                 http://sage.int.janelia.org/sage-ws/lines/rubin/NP6250.janelia-sage,
                                 http://sage.int.janelia.org/sage-ws/lines/rubin/NP6099.janelia-sage,
                                 http://sage.int.janelia.org/sage-ws/lines/rubin/NP7226.janelia-sage,
                                 http://sage.int.janelia.org/sage-ws/lines/rubin/NP7281.janelia-sage,
                                 http://sage.int.janelia.org/sage-ws/lines/rubin/NP7476.janelia-sage"/>
            </rowValidator>

            <!-- TODO: handle Simpson line c155_Elav -->
            <rowListener className="org.janelia.it.ims.tmog.plugin.imagedb.ImageDataPlugin">
                <property name="db.config.key" value="sage"/>
                <property name="family" value="$STATIC:rubin_wu"/>
                <property name="lab" value="$STATIC:rubin"/>
                <property name="line" value="${Line 1 (select)}${Line 1 (text)}"/>
                <property name="mounting_protocol" value="Tissue Mounting Protocol"/>
                <property name="tissue_orientation" value="Tissue Orientation"/>
            </rowListener>

        </plugins>

        <dataFields>
            <separator value="RDSP-"/>
            <validValueList displayName="Line 1 (select)">
                <validValue value="GMR_12H12_AE_01"/>
                <validValue value="GMR_13D05_AE_01"/>
                <validValue value="GMR_14A11_AE_01"/>
                <validValue value="GMR_14A11_LJ_21"/>
            </validValueList>
            <text displayName="Line 1 (text)" displayWidth="150">
            </text>
            <separator value="-"/>
            <validValueList displayName="UAS Reporter 1" required="true">
                <validValue displayName="r1a: pJFRC12-10XUAS-IVS-myr::GFP at su(Hw)attP5" value="r1a" default="true"/>
                <validValue displayName="r1b: pJFRC12-10XUAS-IVS-myr::GFP at attP2" value="r1b" />
                <validValue displayName="r1c: pJFRC-13XsulA-IVS-myr::smGFP-V5-DARK at su(Hw)attP8" value="r1c" />
            </validValueList>
            <separator value="-"/>
            <validValueList displayName="Line 2 (select)">
                <validValue value="16H08_KD_GS4_P2"/>
                <validValue value="29C07_KD_GS4_P2"/>
                <validValue value="29C07_KD_GS4_P40"/>
                <validValue value="29C07_KD_GS4_P2_P40"/>
                <validValue value="40E09_KD_GS4_P2"/>
                <validValue value="NP0681"/>
                <validValue value="NP1035"/>
                <validValue value="NP3045"/>
                <validValue value="NP6250"/>
                <validValue value="NP6099"/>
                <validValue value="NP7226"/>
                <validValue value="NP7281"/>
                <validValue value="NP7476"/>
            </validValueList>
            <text displayName="Line 2 (text)" displayWidth="150">
            </text>
            <separator value="-"/>
            <validValueList displayName="UAS Reporter 2" required="true">
                <validValue displayName="r2a: pJFRC-UAS-KDRT-STOP-KDRT-myr::RFP at attP40" value="r2a" default="true"/>
                <validValue displayName="r2b: pJFRC-UAS-IVS-myr::smGFP-HA-DARK at attP18" value="r2b" default="true"/>
            </validValueList>
            <separator value="-"/>
            <validValueList displayName="Gender" required="true">
                <validValue displayName="m: Male" value="m"/>
                <validValue displayName="f: Female" value="f" default="true"/>
                <validValue displayName="x: Not Defined" value="x"/>
            </validValueList>
            <validValueList displayName="Age" required="true">
                <validValue displayName="X00: undefined" value="X00"/>
                <validValue displayName="P01: Prepupa" value="P01"/>
                <validValue displayName="P02: Pupa" value="P02"/>
                <validValue displayName="E01: Embryonic Stage 1" value="E01"/>
                <validValue displayName="E02: Embryonic Stage 2" value="E02"/>
                <validValue displayName="E03: Embryonic Stage 3" value="E03"/>
                <validValue displayName="E04: Embryonic Stage 4" value="E04"/>
                <validValue displayName="E05: Embryonic Stage 5" value="E05"/>
                <validValue displayName="E06: Embryonic Stage 6" value="E06"/>
                <validValue displayName="E07: Embryonic Stage 7" value="E07"/>
                <validValue displayName="E08: Embryonic Stage 8" value="E08"/>
                <validValue displayName="E09: Embryonic Stage 9" value="E09"/>
                <validValue displayName="E10: Embryonic Stage 10" value="E10"/>
                <validValue displayName="E11: Embryonic Stage 11" value="E11"/>
                <validValue displayName="E12: Embryonic Stage 12" value="E12"/>
                <validValue displayName="E13: Embryonic Stage 13" value="E13"/>
                <validValue displayName="E14: Embryonic Stage 14" value="E14"/>
                <validValue displayName="E15: Embryonic Stage 15" value="E15"/>
                <validValue displayName="E16: Embryonic Stage 16" value="E16"/>
                <validValue displayName="E17: Embryonic Stage 17" value="E17"/>
                <validValue displayName="L01: First Instar" value="L01"/>
                <validValue displayName="L02: Second Instar" value="L02"/>
                <validValue displayName="L03: Third Instar" value="L03"/>
                <validValue displayName="A01: Day 1-5" value="A01"/>
                <validValue displayName="A02: Day 6-10" value="A02" default="true"/>
                <validValue displayName="A03: Day 11-15" value="A03"/>
                <validValue displayName="A04: Day 16-20" value="A04"/>
                <validValue displayName="A05: Day 21-25" value="A05"/>
                <validValue displayName="A06: Day 26-30" value="A06"/>
            </validValueList>
            <validValueList displayName="Area" required="true">
                <validValue displayName="b: Brain" value="b" default="true"/>
                <validValue displayName="s: Salivary Gland" value="s"/>
                <validValue displayName="v: Ventral Nerve Cord" value="v"/>
            </validValueList>
            <separator value="-"/>
            <number displayName="Concentration" required="true"
                    minimumValue="0" maximumValue="5000"  padFormat="40">
                <sourceFileDefault pattern=".+L\d\d??_.*-(\d+).*#.*"/>                
            </number>
            <separator value="-"/>
            <number displayName="Sample Number" required="true"
                    minimumValue="1" maximumValue="99"  padFormat="20">
            </number>
            <separator value="-"/>
            <runTime datePattern="yyyyMMddHHmmssSSS"/>
            <separator value=".lsm"/>

            <!--
             These values are collected for the SAGE database but are not
             included as components of the file name (markedForTask="false").
             -->
            <validValueList displayName="Tissue Mounting Protocol" required="true"
                            markedForTask="false">
                <validValue value="Glycerol-Based Mounting" default="true"/>
                <validValue value="DPX Mounting"/>
            </validValueList>
            <cvTermList displayName="Tissue Orientation" required="true"
                        markedForTask="false"
                        serviceUrl="http://sage.int.janelia.org/sage-ws/cvs/tissue_orientation"
                        displayNamePrefixedForValues="false">
                <staticDefault value="face_up"/>                
            </cvTermList>
        </dataFields>

    </project>

</transmogrifierConfiguration>