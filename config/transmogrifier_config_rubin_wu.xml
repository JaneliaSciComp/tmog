    <project name="Rubin Lab: Wu Split-Gal4 Images (wu-split_*)" default="true">

        <inputFileSorter sortAlgorithm="LNumber" />

        <outputDirectory basePath="[//tier2.hhmi.org|/tier2]/rubin/data1/rubinlab/Ming/rubin_wu_split/confocalStacks"
                         derivedFromEarliestModifiedFile="true"/>

        <plugins>

            <rowListener className="org.janelia.it.ims.tmog.plugin.imagedb.ImageDataPlugin">
                <property name="db.config.key" value="sage"/>
                <property name="family" value="$STATIC:rubin_wu_split"/>
                <property name="lab" value="$STATIC:none"/>
                <property name="line" value="$STATIC:Not_Applicable"/>
                <property name="display" value="$STATIC:false"/>
                <property name="base_path" value="$STATIC:/tier2/rubin/data1/rubinlab/Ming/rubin_wu_split/confocalStacks/"/>
                <property name="activation_line" value="${AD Line}-p65ADZp-in-${AD Landing Site}"/>
                <property name="binding_line" value="${DBD Line}-ZpGdbd-in-${DBD Landing Site}"/>
                <property name="gender" value="Gender$MAP:m=Male|f=Female|x=unknown"/>
                <property name="area" value="Area$MAP:b=Brain|v=Ventral Nerve Cord"/>
                <property name="age" value="Age"/>
                <property name="cross_number" value="Cross Number"/>
                <property name="anatomical_region" value="Brain Region"/>
                <property name="mounting_protocol" value="Tissue Mounting Protocol"/>
                <property name="tissue_orientation" value="Tissue Orientation"/>
            </rowListener>

        </plugins>

        <dataFields>

            <separator value="wu-split_"/>

            <text displayName="Cross Number"
                  required="true"
                  pattern="[0-9A-Za-z\-]++"
                  maximumLength="30">
            </text>

            <separator value="_"/>

            <!-- Activation Domain -->
            <text displayName="AD Line"
                  required="true"
                  pattern="[0-9A-Za-z]++"
                  maximumLength="5">
            </text>

            <separator value="-p65ADZp-"/>

            <validValueList displayName="AD Landing Site"
                            required="true">
                <validValue value="attP40" default="true"/>
                <validValue value="suHw-attP8"/>
                <validValue value="VK00027"/>
            </validValueList>

            <separator value="_"/>

            <!-- DNA Binding Domain -->
            <text displayName="DBD Line"
                  required="true"
                  pattern="[0-9A-Za-z]++"
                  maximumLength="10">
            </text>

            <separator value="-ZpGdbd-"/>

            <validValueList displayName="DBD Landing Site"
                            required="true">
                <validValue value="attP2" default="true"/>
            </validValueList>

            <separator value="_"/>
            <validValueList displayName="Reporter"
                            required="true">
                <validValue displayName="r1a: JFRC12-10XUAS-myr-GFP in attP2" value="r1a"/>
                <validValue displayName="r1b: pJFRC-IVS- myr-smGFP-HA-DARK in attP18, pJFRC-IVS-myr-smGFP-V5-DARK in su(Hw)attP8" value="r1b" default="true"/>
                <validValue displayName="r1c: pJFRC2-10XUAS-IVS-mCD8::GFP in VK00005" value="r1c"/>
                <validValue displayName="r1d: pJFRC101-20XUAS-IVS-Syn21-Shibire-ts1-GFP-p10 in VK00005" value="r1d"/>
                <validValue displayName="r1e: pJFRC19-13XLexAop2-IVS-myr::GFP in VK00005" value="r1d"/>
            </validValueList>

            <separator value="_"/>

            <validValueList displayName="Gender" required="true">
                <validValue displayName="f: Female" value="f" default="true"/>
                <validValue displayName="m: Male" value="m"/>
                <validValue displayName="x: Not Defined" value="x"/>
            </validValueList>

            <cvTermList displayName="Age" required="true"
                        serviceUrl="http://sage.int.janelia.org/sage-ws/cvs/age_remap"
                        displayNamePrefixedForValues="true"
                        markedForTask="true">
                <staticDefault value="A02"/>
            </cvTermList>

            <validValueList displayName="Area" required="true">
                <validValue displayName="b: Brain" value="b" default="true"/>
                <validValue displayName="v: Ventral Nerve Cord" value="v"/>
            </validValueList>

            <separator value="_"/>

            <number displayName="Sample Number" required="true"
                    minimumValue="1" maximumValue="99"  padFormat="20">
            </number>

            <separator value="_"/>
            <runTime datePattern="yyyyMMddHHmmssSSS"/>
            <separator value=".lsm"/>

            <!--
             These values are collected for the SAGE database but are not
             included as components of the file name (markedForTask="false").
             -->
            <validValueList displayName="Tissue Mounting Protocol" required="true"
                            markedForTask="false">
                <validValue value="Glycerol-Based Mounting"/>
                <validValue value="DPX Mounting" default="true"/>
            </validValueList>

            <cvTermList displayName="Tissue Orientation" required="true"
                        markedForTask="false"
                        serviceUrl="http://sage.int.janelia.org/sage-ws/cvs/tissue_orientation"
                        displayNamePrefixedForValues="false">
                <staticDefault value="unknown"/>
            </cvTermList>

            <validValueList displayName="Brain Region" required="true"
                            markedForTask="false">
                <validValue value="mushroom body"/>
                <validValue value="central complex"/>
                <validValue value="optic lobe"/>
                <validValue value="vlpr" default="true"/>
            </validValueList>

        </dataFields>

    </project>

