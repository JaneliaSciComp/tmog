                <validValue displayName="v: Ventral Nerve Cord" value="v"/>
            </validValueList>
            <separator value="-"/>
            <text displayName="Concentration" required="true"
                  pattern="[0-9A-Za-z\_]++" padFormat="40">
                <sourceFileDefault pattern=".+L\d\d??_.*-(\d+).*#.*"/>                
            </text>
            <separator value="-"/>
            <number displayName="Sample Number" required="true"
                    minimumValue="1" maximumValue="99"  padFormat="20">
            </number>
            <separator value="-"/>
            <runTime datePattern="yyyyMMddHHmmssSSS"/>
            <separator value=".lsm"/>

