<?xml version="1.0"?>

<transmogrifierConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                     xsi:noNamespaceSchemaLocation="transmogrifier_config.xsd">

    <project name="Lee Lab: ChaCRM verified TransformantIDs (TZL_*)" default="true">

        <inputFileSorter sortAlgorithm="LNumber" />

        <outputDirectory>
            <path path="\\dm3.int.janelia.org\leetconfocal\confocalStacks\" />
            <renameFieldValue fieldDisplayName="Plate" prefix="GMR_"/>
            <renameFieldValue fieldDisplayName="Well" suffix="_"/>
            <renameFieldValue fieldDisplayName="Vector ID" suffix="_"/>
            <renameFieldValue fieldDisplayName="Landing Site" />
        </outputDirectory>

        <plugins>
            <rowValidator className="org.janelia.it.chacrm.ChacrmEventManager" />
            <sessionListener className="org.janelia.it.ims.tmog.plugin.SessionSummaryFileWriter">
                <property name="directory" value="\\dm3.int.janelia.org\leetconfocal\tmog\summaries\TZL"/>
            </sessionListener>
        </plugins>

        <dataFields>
            <separator value="TZL_"/>
            <number displayName="Plate" required="true"
                    minimumValue="1" maximumValue="999"/>
            <well displayName="Well" required="true" formFactor="96"/>
            <separator value="_"/>
            <validValueList displayName="Vector ID" required="true">
                <validValue displayName="AA: pBPGToyw" value="AA"/>
                <validValue displayName="AB: pBPGEarw" value="AB"/>
                <validValue displayName="AC: pBPGDacw" value="AC"/>
                <validValue displayName="AD: pBPGw" value="AD"/>
                <validValue displayName="AE: pBPGUw" value="AE"/>
                <validValue displayName="AF: pBPGUv" value="AF"/>
                <validValue displayName="AG: pBPLxVUw-1" value="AG"/>
                <validValue displayName="AH: pBPLxVw-1" value="AH"/>
                <validValue displayName="AI: pBPLxVUw-2" value="AI"/>
                <validValue displayName="AJ: pBPLxVw-2" value="AJ"/>
                <validValue displayName="AK: pBPLxGUw" value="AK"/>
                <validValue displayName="AL: pBPLxGw" value="AL"/>
                <validValue displayName="AM: pBPLxPUw" value="AM"/>
                <validValue displayName="AN: pBPLxPw" value="AN"/>
                <validValue displayName="AO: pBPGfpUw" value="AO"/>
                <validValue displayName="AP: pBPGfpw" value="AP"/>
                <validValue displayName="AQ: pBPGhERUw" value="AQ"/>
                <validValue displayName="AR: pBPGhERw" value="AR"/>
                <validValue displayName="AS: pBPSwitchUw" value="AS"/>
                <validValue displayName="AT: pBPSwitchw" value="AT"/>
            </validValueList>
            <separator value="_"/>
            <validValueList displayName="Landing Site" required="true">
                <validValue displayName="01: attP2" value="01" default="true"/>
                <validValue displayName="02: attP40-2nd" value="02"/>
                <validValue displayName="03: attP16-2R" value="03"/>
                <validValue displayName="04: attP3-X" value="04"/>
                <validValue displayName="05: attP83-CyO" value="05"/>
                <validValue displayName="06: su(Hw)attP1-3rd " value="06"/>
                <validValue displayName="07: su(Hw)attP2-3rd " value="07"/>
                <validValue displayName="08: su(Hw)attP4-3rd " value="08"/>
                <validValue displayName="09: su(Hw)attP5-2nd " value="09"/>
                <validValue displayName="10: su(Hw)attP6-2nd " value="10"/>
                <validValue displayName="11: su(Hw)attP7-3rd " value="11"/>
                <validValue displayName="12: su(Hw)attP8-X" value="12"/>
            </validValueList>

            <separator value="-"/>
            <validValueList displayName="Gender" required="true">
                <validValue displayName="m: Male" value="m"/>
                <validValue displayName="f: Female" value="f"/>
                <validValue displayName="x: Not Defined" value="x"/>
            </validValueList>
            <validValueList displayName="Age" required="true">
                <validValue displayName="X00: undefined" value="X00"/>
                <validValue displayName="P01: Prepupa" value="P01"/>
                <validValue displayName="P02: Pupa" value="P02"/>
                <validValue displayName="E01: Embryonic Stage 1" value="E01"/>
                <validValue displayName="E02: Embryonic Stage 2" value="E02"/>
                <validValue displayName="E03: Embryonic Stage 3" value="E03"/>
                <validValue displayName="E04: Embryonic Stage 4" value="E04"/>
                <validValue displayName="E05: Embryonic Stage 5" value="E05"/>
                <validValue displayName="E06: Embryonic Stage 6" value="E06"/>
                <validValue displayName="E07: Embryonic Stage 7" value="E07"/>
                <validValue displayName="E08: Embryonic Stage 8" value="E08"/>
                <validValue displayName="E09: Embryonic Stage 9" value="E09"/>
                <validValue displayName="E10: Embryonic Stage 10" value="E10"/>
                <validValue displayName="E11: Embryonic Stage 11" value="E11"/>
                <validValue displayName="E12: Embryonic Stage 12" value="E12"/>
                <validValue displayName="E13: Embryonic Stage 13" value="E13"/>
                <validValue displayName="E14: Embryonic Stage 14" value="E14"/>
                <validValue displayName="E15: Embryonic Stage 15" value="E15"/>
                <validValue displayName="E16: Embryonic Stage 16" value="E16"/>
                <validValue displayName="E17: Embryonic Stage 17" value="E17"/>
                <validValue displayName="L01: First Instar" value="L01"/>
                <validValue displayName="L02: Second Instar" value="L02"/>
                <validValue displayName="L03: Third Instar" value="L03"/>
                <validValue displayName="A01: Day 1-5" value="A01"/>
                <validValue displayName="A02: Day 6-10" value="A02"/>
                <validValue displayName="A03: Day 11-15" value="A03"/>
                <validValue displayName="A04: Day 16-20" value="A04"/>
                <validValue displayName="A05: Day 21-25" value="A05"/>
                <validValue displayName="A06: Day 26-30" value="A06"/>
            </validValueList>
            <validValueList displayName="Area" required="true">
                <validValue displayName="b: Brain" value="b"/>
                <validValue displayName="v: Ventral Nerve Cord" value="v"/>
            </validValueList>

            <separator value="_"/>
            <validValueList displayName="Heat Shock Age" required="true">
                <validValue displayName="X00: undefined" value="X00" default="true"/>
                <validValue displayName="E: embryo" value="E"/>
                <validValue displayName="L11: 1 day larva" value="L11"/>
                <validValue displayName="L12: 2 day larva" value="L12"/>
                <validValue displayName="L13: 3 day larva" value="L13"/>
                <validValue displayName="L14: 4 day larva" value="L14"/>
                <validValue displayName="L15: 5 day larva" value="L15"/>
                <validValue displayName="L16: 6 day larva" value="L16"/>
                <validValue displayName="L17: 7 day larva" value="L17"/>
                <validValue displayName="L18: 8 day larva" value="L18"/>
                <validValue displayName="L19: 9 day larva" value="L19"/>
                <validValue displayName="P11: 1 day pupa" value="P11"/>
                <validValue displayName="P12: 2 day pupa" value="P12"/>
                <validValue displayName="P13: 3 day pupa" value="P13"/>
                <validValue displayName="P14: 4 day pupa" value="P14"/>
                <validValue displayName="P15: 5 day pupa" value="P15"/>
                <validValue displayName="P16: 6 day pupa" value="P16"/>
                <validValue displayName="P17: 7 day pupa" value="P17"/>
                <validValue displayName="P18: 8 day pupa" value="P18"/>
            </validValueList>
            <separator value="_"/>
            <number displayName="Heat Shock Minute" required="true"
                    minimumValue="0" maximumValue="90" >
                <staticDefault value="0"/>
            </number>

            <separator value="_"/>
            <runTime datePattern="yyyyMMddHHmmssSSS"/>
            <separator value=".lsm"/>

        </dataFields>

    </project>

</transmogrifierConfiguration>