@config-fragment-tmog-begin@
@config-fragment-global@
    <project name="Fly Light - MCFO - scope to confocalStacks" default="true">

@config-fragment-lsm-input@
@config-fragment-lsm-sort@
@config-fragment-output-flip@
        <plugins>

@config-fragment-update-mapped-serial-data@
@config-fragment-update-project-cross-data@
@config-fragment-update-zeiss-lsm-data@
@config-fragment-correct-objective@
@config-fragment-validate-common-fields@
@config-fragment-image-sequence-flip@
            <rowListener className="org.janelia.it.ims.tmog.plugin.imagedb.ImageDataPlugin">
@config-fragment-common-image-data-properties@
                <property name="display" value="Screen State$MAP:accepted=true|accepted_public=true|pre_screen=false|rejected=false"/>
                <property name="family" value="$STATIC:flylight_flip"/>
                <property name="screen_state" value="Screen State"/>
            </rowListener>

@config-fragment-sage-loader-flip@
@config-fragment-copy-zeiss-log-files@
        </plugins>

        <dataFields>

            <separator value="FLFL_"/>
@config-fragment-file-name-suffix@
@config-fragment-annotated_by@
@config-fragment-data_set@
@config-fragment-tile@
@config-fragment-slide_code@
@config-fragment-cross_barcode@
@config-fragment-line@
@config-fragment-effector@
@config-fragment-gender@
@config-fragment-age@
@config-fragment-full_age@
@config-fragment-heat_shock_minutes@
@config-fragment-created_by@
@config-fragment-mounting_protocol@
@config-fragment-tissue_orientation@
@config-fragment-screen_state@
@config-fragment-imaging_project-mcfo@
@config-fragment-objective@
@config-fragment-channel-data@
        </dataFields>

    </project>

    <project name="Fly Light - MCFO - fix data" taskName="collector">

@config-fragment-lsm-bz2-input@
@config-fragment-lsm-sort@
        <plugins>

@config-fragment-update-mapped-serial-data@
@config-fragment-update-project-cross-data@
@config-fragment-image-path-exists@
            <rowListener className="org.janelia.it.ims.tmog.plugin.imagedb.ImageDataPlugin">
@config-fragment-common-image-data-properties@
                <property name="comment" value="Comment"/>
                <property name="display" value="Screen State$MAP:accepted=true|accepted_public=true|pre_screen=false|rejected=false"/>
                <property name="exclude.host.name" value="true"/>
                <property name="family" value="$STATIC:flylight_flip"/>
                <property name="keep.existing.data" value="true"/>
                <property name="relative_path" value="SAGE Name"/>
                <property name="screen_state" value="Screen State"/>
            </rowListener>

        </plugins>

        <dataFields>

@config-fragment-annotated_by-with-flylight_flip-default@
@config-fragment-data_set-with-flylight_flip-default@
@config-fragment-cross_barcode-with-flylight_flip-default@
@config-fragment-line-with-flylight_flip-default@
@config-fragment-effector-with-flylight_flip-default@
@config-fragment-slide_code-with-flylight_flip-default@
@config-fragment-tile-with-flylight_flip-default@
@config-fragment-screen_state-with-flylight_flip-default@
@config-fragment-comment-with-flylight_flip-default@
@config-fragment-gender-with-flylight_flip-default@
@config-fragment-age-with-flylight_flip-default@
@config-fragment-full_age-with-flylight_flip-default@
@config-fragment-heat_shock_minutes-with-flylight_flip-default@
@config-fragment-created_by-with-flylight_flip-default@
@config-fragment-mounting_protocol-with-flylight_flip-default@
@config-fragment-tissue_orientation-with-flylight_flip-default@
@config-fragment-imaging_project-with-flylight_flip-default@
@config-fragment-name-with-flylight_flip-default@
        </dataFields>

    </project>

    <project name="Fly Light Flip - promote preScreen to confocalStacks">

        <inputFileFilter patternString="^((?!FLAG_3_0048__).)*\.lsm$" />

        <!-- NOTES:
             - first capturing group must be equal for second capturing group
               to be used in numeric sort
             - third capturing group only used when first and second are equal
             - '(?: )' identifies a non-capturing group
        -->
        <inputFileSorter sortAlgorithm="Number"
                         patternString=".*_(\d{8}_\d\d_[A-Z]\d).*_R._L(\d++)_(.*)"/>

@config-fragment-output-flip@
        <plugins>

            <rowUpdater className="org.janelia.it.ims.tmog.plugin.dataFile.TileSetterPlugin">
            </rowUpdater>

@config-fragment-update-mapped-serial-data@
@config-fragment-update-project-cross-data@
@config-fragment-update-zeiss-lsm-data@
@config-fragment-correct-objective@
@config-fragment-validate-common-fields@
@config-fragment-image-sequence-flip@
            <rowListener className="org.janelia.it.ims.tmog.plugin.imagedb.ImageDataPlugin">
@config-fragment-common-image-data-properties@
                <property name="days_after_eclosion" value="Days After Eclosion"/>
                <property name="exclude.host.name" value="true"/>
                <property name="family" value="$STATIC:flylight_flip"/>
                <property name="screen_state" value="$STATIC:accepted"/>
            </rowListener>

@config-fragment-sage-loader-flip@
@config-fragment-copy-zeiss-log-files@
        </plugins>

        <dataFields>

            <separator value="FLFL_"/>
@config-fragment-hidden-file-name-suffix@
@config-fragment-annotated_by@
            <webServiceList displayName="Data Set" required="true"
                            markedForTask="false"
                            sharedForAllSessionFiles="true"
                            valueCreationPath="*/dataSet"
                            relativeActualValuePath="dataSetIdentifier"
                            relativeValueDisplayNamePath="dataSetIdentifier"
                            serviceUrl="http://jacs.int.janelia.org:8180/rest-v1/dataSet?user=user:asoy"
                            autoComplete="true">
                <staticDefault value="asoy_mb_split_mcfo_case_1"/>
            </webServiceList>

            <number displayName="Cross Serial Number"
                    minimumValue="1"
                    maximumValue="99999"
                    markedForTask="false">
                <sourceFileDefault pattern=".*(\d{5})_\d{8}_\d{2}_.*" />
            </number>

@config-fragment-line@
@config-fragment-effector@
            <text displayName="Slide Code" required="true"
                  pattern="\d{8}_\d+_[A-Z]\d+"
                  copyable="false"
                  markedForTask="false">
                <sourceFileDefault pattern=".*_(\d{8}_\d+_[A-Z]\d+).*"/>
                <sourceFileDefault pattern=".*_(\d{8}_\d+_).*"/> <!-- for preScreen prior to zeiss brain number standardization -->
            </text>

            <cvTermList displayName="Tile" required="true"
                        serviceUrl="http://sage.int.janelia.org/sage-ws/cvs/fly_light_adult_tiles"
                        autoComplete="true"
                        markedForTask="false">
            </cvTermList>

@config-fragment-gender@
@config-fragment-age@
@config-fragment-full_age@
            <number displayName="Days After Eclosion" required="false"
                    markedForTask="false"
                    minimumValue="0"
                    maximumValue="21" />

@config-fragment-created_by@
@config-fragment-mounting_protocol@
@config-fragment-tissue_orientation@
            <text displayName="Channel Specification" required="false"
                  markedForTask="false"
                  pattern="s*rs*">
            </text>

@config-fragment-objective@
@config-fragment-imaging_project-mcfo@
        </dataFields>

    </project>

    <project name="Fly Light Cell Count - promote preScreen to confocalStacks">

        <inputFileFilter patternString=".*FLAG_3_0048__.*\.lsm" />

        <!-- NOTES:
             - first capturing group must be equal for second capturing group
               to be used in numeric sort
             - third capturing group only used when first and second are equal
             - '(?: )' identifies a non-capturing group
        -->
        <inputFileSorter sortAlgorithm="Number"
                         patternString=".*_(\d{8}_\d\d_[A-Z]\d).*_R._L(\d++)_(.*)"/>

@config-fragment-output-flip@
        <plugins>

            <rowUpdater className="org.janelia.it.ims.tmog.plugin.dataFile.TileSetterPlugin">
            </rowUpdater>

@config-fragment-update-project-cross-data@
@config-fragment-validate-common-fields@
@config-fragment-image-sequence-flip@
            <rowListener className="org.janelia.it.ims.tmog.plugin.imagedb.ImageDataPlugin">
@config-fragment-common-image-data-properties@
                <property name="area" value="$STATIC:Brain"/>
                <property name="exclude.host.name" value="true"/>
                <property name="family" value="$STATIC:flylight_flip"/>
                <property name="imaging_project" value="$STATIC:cell_count"/>
                <property name="screen_state" value="$STATIC:accepted"/>
            </rowListener>

@config-fragment-sage-loader-flip@
@config-fragment-copy-zeiss-log-files@
        </plugins>

        <dataFields>

            <separator value="FLFL_"/>
@config-fragment-hidden-file-name-suffix@
@config-fragment-annotated_by@
            <webServiceList displayName="Data Set" required="true"
                            markedForTask="false"
                            sharedForAllSessionFiles="true"
                            valueCreationPath="*/dataSet"
                            relativeActualValuePath="dataSetIdentifier"
                            relativeValueDisplayNamePath="dataSetIdentifier"
                            serviceUrl="http://jacs.int.janelia.org:8180/rest-v1/dataSet?user=user:asoy"
                            autoComplete="true">
                <staticDefault value="asoy_cell_count"/>
            </webServiceList>

            <number displayName="Cross Serial Number"
                    minimumValue="1"
                    maximumValue="99999"
                    markedForTask="false">
                <sourceFileDefault pattern=".*(\d{5})_\d{8}_\d{2}_.*" />
            </number>

@config-fragment-line@
            <!--
                 For cell counting, reporter should always be UAS_nls_myr-FLAG_3_0048.
                 This ensures reporter loaded from cross serial number is correct.
            -->
            <text displayName="Reporter" required="true"
                  pattern="UAS_nls_myr-FLAG_3_0048"
                  markedForTask="false">
            </text>

            <text displayName="Slide Code" required="true"
                  pattern="\d{8}_\d+_[A-Z]\d+"
                  copyable="false"
                  markedForTask="false">
                <sourceFileDefault pattern=".*_(\d{8}_\d+_[A-Z]\d+).*"/>
                <sourceFileDefault pattern=".*_(\d{8}_\d+_).*"/> <!-- for preScreen prior to zeiss brain number standardization -->
            </text>

            <cvTermList displayName="Tile" required="true"
                        serviceUrl="http://sage.int.janelia.org/sage-ws/cvs/fly_light_adult_tiles"
                        autoComplete="true"
                        markedForTask="false">
            </cvTermList>

@config-fragment-gender@
@config-fragment-age@
            <cvTermList displayName="Imager" required="true"
                        markedForTask="false"
                        autoComplete="true"
                        serviceUrl="http://sage.int.janelia.org/sage-ws/cvs/imager">
                <staticDefault value="Nirmala Iyer" />
            </cvTermList>

@config-fragment-mounting_protocol@
@config-fragment-tissue_orientation@
        </dataFields>

    </project>

    <project name="Fly Light - MCFO - run sageLoader" taskName="collector">

@config-fragment-lsm-bz2-input@
        <plugins>

@config-fragment-image-path-exists@
@config-fragment-sage-loader-flip@
        </plugins>

        <dataFields>

@config-fragment-data_set-with-flylight_flip-default@
@config-fragment-slide_code-with-flylight_flip-default@
@config-fragment-tile-with-flylight_flip-default@
            <text displayName="Capture Date" required="false"
                  markedForTask="false"
                  editable="false">
                <pluginDefault className="org.janelia.it.ims.tmog.plugin.imagedb.SageImageDataDefaultValue">
                    <property name="family" value="flylight_flip"/>
                    <property name="image_property" value="capture_date"/>
                    <property name="baseNameSearch" value="true"/>
                </pluginDefault>
            </text>

@config-fragment-name-with-flylight_flip-default@
        </dataFields>

    </project>

@config-fragment-tmog-end@