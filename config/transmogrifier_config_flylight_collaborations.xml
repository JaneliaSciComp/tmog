<?xml version="1.0"?>

<transmogrifierConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                     xsi:noNamespaceSchemaLocation="transmogrifier_config.xsd">

    <global minimumVersion="2.8.7" frameSizePercentage="95"/>

    <project name="FlyLight Collaborations" default="true">

        <inputFileSorter sortAlgorithm="LNumber" />

        <outputDirectory basePath="[\\dm11.janelia.priv|/groups/flylight]/flylight/collaborations/confocalStacks"
                         derivedFromEarliestModifiedFile="true"/>

        <plugins>

            <rowListener className="org.janelia.it.ims.tmog.plugin.imagedb.ImageSequencePlugin">
                <property name="db.config.key" value="sage" />
                <property name="image_number" value="urn:lsid:janelia.org:flylight_collaborations:image_number" />
            </rowListener>

            <rowListener className="org.janelia.it.ims.tmog.plugin.imagedb.ImageDataPlugin">
                <property name="db.config.key" value="sage"/>
                <property name="lab" value="Line Lab"/>
                <property name="family" value="$STATIC:flylight_collaborations"/>
                <property name="line" value="Line"/>
                <property name="gender" value="Gender"/>
                <property name="age" value="Age"/>
                <property name="chron_interval" value="Hours After Pupa Formation"/>
                <property name="area" value="Area"/>
                <property name="stain_antibody" value="Stain Antibody"/>
                <property name="mounting_protocol" value="Tissue Mounting Protocol"/>
                <property name="tissue_orientation" value="Tissue Orientation"/>
                <property name="effector" value="UAS Reporter"/>
                <property name="created_by" value="Imager"/>
                <property name="renamed_by" value="Renamer"/>
                <property name="class" value="Class"/>
            </rowListener>

            <sessionListener className="org.janelia.it.ims.tmog.plugin.SessionSummaryFileWriter">
                <property name="directory" value="[\\dm11.janelia.priv|/groups/flylight]/flylight/tmog/summaries/collaborations"/>
            </sessionListener>

        </plugins>

        <dataFields>

            <separator value="FLCO_"/>
            <runTime datePattern="yyyyMMddHHmmssSSS" />
            <separator value="_"/>
            <pluginData displayName="image_number"/>
            <separator value=".lsm"/>

            <!--
             These values are collected for storage but are not
             included as components of the file name (markedForTask="false").
             -->

            <cvTermList displayName="Line Lab" required="true"
                        serviceUrl="http://sage.int.janelia.org/sage-ws/cvs/lab"
                        displayNamePrefixedForValues="false"
                        markedForTask="false">
                <staticDefault value="olympiad" />
            </cvTermList>

            <webServiceList displayName="Line" required="true"
                            markedForTask="false"
                            valueCreationPath="*/line"
                            relativeActualValuePath="name"
                            relativeValueDisplayNamePath="name"
                            serviceUrl="http://sage.int.janelia.org/sage-ws/lines/forMultipleLabs.janelia-sage?lab=baker&amp;lab=fly&amp;lab=flylight&amp;lab=olympiad&amp;lab=jayaraman&amp;lab=rubin&amp;lab=simpson&amp;lab=leet&amp;lab=truman&amp;lab=zlatic"
                            autoComplete="true">
            </webServiceList>

            <validValueList displayName="Gender" required="true"
                            markedForTask="false">
                <validValue value="Female"/>
                <validValue value="Male"/>
                <validValue value="unknown"/>
            </validValueList>

            <validValueList displayName="Age" required="true" markedForTask="false">
                <validValue value="undefined"/>
                <validValue value="Prepupa"/>
                <validValue value="Pupa"/>
                <validValue value="First Instar"/>
                <validValue value="Second Instar"/>
                <validValue value="Third Instar"/>
                <validValue value="Day 1-5"/>
                <validValue value="Day 6-10"/>
                <validValue value="Day 11-15"/>
                <validValue value="Day 16-20"/>
                <validValue value="Day 21-25"/>
                <validValue value="Day 26-30"/>
            </validValueList>

            <number displayName="Hours After Pupa Formation" required="false"
                    minimumValue="0" maximumValue="100"
                    markedForTask="false">
            </number>

            <validValueList displayName="Area" required="true"
                            markedForTask="false">
                <validValue value="Brain" />
                <validValue value="Salivary Gland" />
                <validValue value="Ventral Nerve Cord" />
            </validValueList>

            <cvTermList displayName="Stain Antibody" required="true"
                        markedForTask="false"
                        displayNamePrefixedForValues="true"
                        serviceUrl="http://sage.int.janelia.org/sage-ws/cvs/antibody">
                <staticDefault value="NC82"/>
            </cvTermList>

            <validValueList displayName="Tissue Mounting Protocol" required="true"
                            markedForTask="false">
                <validValue value="Glycerol-Based Mounting" default="true"/>
                <validValue value="DPX Mounting"/>
            </validValueList>

            <cvTermList displayName="Tissue Orientation" required="true"
                        markedForTask="false"
                        serviceUrl="http://sage.int.janelia.org/sage-ws/cvs/tissue_orientation"
                        displayNamePrefixedForValues="false">
            </cvTermList>

            <validValueList displayName="UAS Reporter" required="true"
                            markedForTask="false">
                <validValue value="UAS_IVS-mCD8_3_0007" default="true"/>
                <validValue value="UAS_mCD8_2_0006"/>
                <validValue value="LexAop_IVS-myr_3_0008"/>
            </validValueList>

            <cvTermList displayName="Imager" required="true"
                        markedForTask="false"
                        autoComplete="true"
                        serviceUrl="http://sage.int.janelia.org/sage-ws/cvs/imager">
            </cvTermList>

            <cvTermList displayName="Renamer" required="true"
                        markedForTask="false"
                        autoComplete="true"
                        serviceUrl="http://sage.int.janelia.org/sage-ws/cvs/imager"
                        sharedForAllSessionFiles="true">
            </cvTermList>

            <validValueList displayName="Class" required="false"
                            markedForTask="false">
                <validValue displayName="Screening" value="Screening" default="true"/>
                <validValue displayName="Rework" value="Rework"/>
            </validValueList>

        </dataFields>

    </project>

    <project name="Fly Light - Test Nikon - MCFO - scope to confocalStacks" taskName="rename-without-delete">

        <inputFileFilter patternString=".+\.lsm" />

        <outputDirectory basePath="[\\dm11.janelia.priv|/groups/flylight]/flylight/collaborations/confocalStacks"
                         derivedFromEarliestModifiedFile="true"/>

        <plugins>

            <rowUpdater className="org.janelia.it.ims.tmog.plugin.dataFile.DataFilePlugin">
                <property name="tmog-row.key" value="${Cross Serial Number}" />
                <property name="data-file.name" value="[\\arch.janelia.priv|/archive]/flylight_archive/tmog/data/mapped-serial-data.tsv" />
                <property name="data-file.tsv-key" value="Serial" />
                <property name="GAL4 Line" value="GAL4 Line" />
            </rowUpdater>

            <rowUpdater className="org.janelia.it.ims.tmog.plugin.dataFile.DataResourcePlugin">
                <property name="plugin.service-url" value="http://sage.int.janelia.org/flyboy-ws/project_crosses/withSerialNumber/${Cross Serial Number}" />
                <property name="plugin.test-url" value="http://sage.int.janelia.org/flyboy-ws/project_crosses/withSerialNumber/51418" />
                <property name="plugin.root-xpath" value="projectCross" />
                <property name="GAL4 Line" value="parent1StockName" />
                <property name="Reporter" value="reporter" />
            </rowUpdater>

            <rowUpdater className="org.janelia.it.ims.tmog.plugin.dataFile.ZeissLsmDataPlugin">
                <property name="plugin-scale" value="0" />
                <property name="Channel Specification" value="Recording #1 Number of Channels$MAP:1=r|2=rs|3=rss" />
                <property name="Channel 1 Name" value="IlluminationChannel #1 Name" />
                <property name="Channel 1 Power" value="IlluminationChannel #1 Power B/C 1" />
                <property name="Channel 1 Gain" value="DetectionChannel #1 Detector Gain" />
                <property name="Channel 2 Name" value="IlluminationChannel #2 Name" />
                <property name="Channel 2 Power" value="IlluminationChannel #2 Power B/C 1" />
                <property name="Channel 2 Gain" value="DetectionChannel #2 Detector Gain" />
                <property name="Channel 3 Name" value="IlluminationChannel #3 Name" />
                <property name="Channel 3 Power" value="IlluminationChannel #3 Power B/C 1" />
                <property name="Channel 3 Gain" value="DetectionChannel #3 Detector Gain" />
            </rowUpdater>

            <rowValidator className="org.janelia.it.ims.tmog.plugin.CrossFieldValidator">
                <property name="validateField" value="Gender"/>
                <property name="matchesPattern" value="\S+"/>
                <property name="whenReferenceField" value="Age"/>
                <property name="matchesReferencePattern" value="^[^L].*"/>
                <property name="errorMessage" value="Gender value is required when age is ${Age}."/>
            </rowValidator>

            <rowListener className="org.janelia.it.ims.tmog.plugin.imagedb.ImageSequencePlugin">
                <property name="db.config.key" value="sage" />
                <property name="image_number" value="urn:lsid:janelia.org:flylight_collaborations:image_number" />
            </rowListener>

            <rowListener className="org.janelia.it.ims.tmog.plugin.imagedb.ImageDataPlugin">
                <property name="db.config.key" value="sage"/>
                <property name="lab" value="$STATIC:flylight"/>
                <property name="family" value="$STATIC:flylight_collaborations"/>
                <property name="imaging_project" value="Imaging Project"/>
                <property name="line" value="GAL4 Line"/>
                <property name="cross_barcode" value="Cross Serial Number"/>
                <property name="created_by" value="Imager"/>
                <property name="effector" value="Reporter"/>
                <property name="slide_code" value="Slide Code"/>
                <property name="tile" value="Tile$MAP:vnc_verify=ventral_nerve_cord" />
                <property name="area" value="Tile$MAP:abdominal=VNC|brain=Brain|brain_with_lamina=Brain|central=Brain|left_central=Brain|left_dorsal=Brain|left_lamina=Brain|left_optic_lobe=Brain|left_optic_lobe_with_lamina=Brain|left_ventral=Brain|mesothorasic=VNC|metathorasic=VNC|prothorasic=VNC|right_central=Brain|right_dorsal=Brain|right_lamina=Brain|right_optic_lobe=Brain|right_optic_lobe_with_lamina=Brain|right_ventral=Brain|ventral=Brain|ventral_nerve_cord=VNC|vnc_verify=VNC-Verify"/>
                <property name="gender" value="Gender"/>
                <property name="age" value="Age"/>
                <property name="full_age" value="Full Age"/>
                <property name="annotated_by" value="Annotator"/>
                <property name="mounting_protocol" value="Tissue Mounting Protocol"/>
                <property name="data_set" value="Data Set"/>
                <property name="channel_spec" value="Channel Specification"/>
                <property name="lsm_detection_channel_1_detector_gain" value="Channel 1 Gain"/>
                <property name="lsm_detection_channel_2_detector_gain" value="Channel 2 Gain"/>
                <property name="lsm_detection_channel_3_detector_gain" value="Channel 3 Gain"/>
                <property name="lsm_illumination_channel_1_name" value="Channel 1 Name"/>
                <property name="lsm_illumination_channel_2_name" value="Channel 2 Name"/>
                <property name="lsm_illumination_channel_3_name" value="Channel 3 Name"/>
                <property name="lsm_illumination_channel_1_power_bc_1" value="Channel 1 Power"/>
                <property name="lsm_illumination_channel_2_power_bc_1" value="Channel 2 Power"/>
                <property name="lsm_illumination_channel_3_power_bc_1" value="Channel 3 Power"/>
                <property name="microscope_filename"/>
                <property name="screen_state" value="Screen State"/>
                <property name="display" value="Screen State$MAP:accepted=true|pre_screen=false|rejected=false"/>
            </rowListener>

            <!--
                  The companion file listener (for log files) should be the
                  last defined plug-in to ensure that log files are only
                  copied if the source lsm processing succeeds.
            -->
            <rowListener className="org.janelia.it.ims.tmog.plugin.CompanionFileRowListener" >
                <property name="sourceSuffix" value=".lsm"/>
                <property name="companionSuffix" value="_log.csv"/>
                <property name="deleteAfterCopy" value="true"/>
            </rowListener>

        </plugins>

        <dataFields>

            <separator value="FLFL_"/>
            <runTime datePattern="yyyyMMddHHmmssSSS" />
            <separator value="_"/>
            <pluginData displayName="image_number"/>
            <separator value=".lsm"/>

            <!--
             These values are collected for storage but are not
             included as components of the file name (markedForTask="false").
             -->

            <webServiceList displayName="Data Set" required="true"
                            markedForTask="false"
                            valueCreationPath="*/dataSet"
                            relativeActualValuePath="dataSetIdentifier"
                            relativeValueDisplayNamePath="dataSetIdentifier"
                            serviceUrl="http://jacs.int.janelia.org:8180/rest-v1/dataSet?user=user:asoy&amp;user=user:nerna&amp;user=user:wolfft"
                            autoComplete="true">
            </webServiceList>

            <cvTermList displayName="Tile" required="true"
                        serviceUrl="http://sage.int.janelia.org/sage-ws/cvs/fly_light_adult_tiles"
                        autoComplete="true"
                        markedForTask="false">
                <sourceFileMappedDefault pattern="^[^_]+_[^_]+_[^_]+_([^_]+)_.*">
                    <mappedValue from="ABD" to="abdominal"/>
                    <mappedValue from="BRN" to="brain"/>
                    <mappedValue from="BRNwLam" to="brain_with_lamina"/>
                    <mappedValue from="CEN" to="central"/>
                    <mappedValue from="DM" to="dorsal_medial"/>
                    <mappedValue from="L" to="left_optic_lobe"/>
                    <mappedValue from="LwLam" to="left_optic_lobe_with_lamina"/>
                    <mappedValue from="LLam" to="left_lamina"/>
                    <mappedValue from="RLam" to="right_lamina"/>
                    <mappedValue from="LamL" to="left_lamina"/>
                    <mappedValue from="LamR" to="right_lamina"/>
                    <mappedValue from="LC" to="left_central"/>
                    <mappedValue from="LD" to="left_dorsal"/>
                    <mappedValue from="LV" to="left_ventral"/>
                    <mappedValue from="MES" to="mesothorasic"/>
                    <mappedValue from="META" to="metathorasic"/>
                    <mappedValue from="NEC-Ant" to="neck_anterior"/>
                    <mappedValue from="NEC-Pos" to="neck_posterior"/>
                    <mappedValue from="PRO" to="prothorasic"/>
                    <mappedValue from="R" to="right_optic_lobe"/>
                    <mappedValue from="RwLam" to="right_optic_lobe_with_lamina"/>
                    <mappedValue from="RC" to="right_central"/>
                    <mappedValue from="RD" to="right_dorsal"/>
                    <mappedValue from="RV" to="right_ventral"/>
                    <mappedValue from="V" to="ventral"/>
                    <mappedValue from="VNC" to="ventral_nerve_cord"/>
                    <mappedValue from="VNC-Adj" to="vnc_verify"/>
                    <mappedValue from="VNC-Ver" to="vnc_verify"/>
                </sourceFileMappedDefault>
                <validValue displayName="VNC-Verify" value="vnc_verify"/>
            </cvTermList>

            <text displayName="Slide Code" required="true"
                  pattern="\d{8}_\d+_[A-Z]\d+"
                  copyable="false"
                  markedForTask="false">
                <sourceFileDefault pattern="^([A-Z]\d+)_ZB.*_(\d{8}_\d+_).*"
                                   patternGroupSpec="2,1"/>
            </text>

            <number displayName="Cross Serial Number"
                    minimumValue="1"
                    maximumValue="99999"
                    markedForTask="false">
            </number>

            <webServiceList displayName="GAL4 Line" required="true"
                            markedForTask="false"
                            valueCreationPath="*/line"
                            relativeActualValuePath="name"
                            relativeValueDisplayNamePath="name"
                            serviceUrl="http://sage.int.janelia.org/sage-ws/lines/forMultipleLabs.janelia-sage?lab=fly&amp;lab=flylight&amp;lab=rubin&amp;lab=jayaraman&amp;lab=simpson&amp;lab=leet&amp;lab=zlatic&amp;lab=dickson"
                            autoComplete="true">
            </webServiceList>

            <cvTermList displayName="Reporter" required="true"
                        autoComplete="true"
                        markedForTask="false"
                        displayNamePrefixedForValues="false"
                        serviceUrl="http://sage.int.janelia.org/sage-ws/cvs/effector">
            </cvTermList>

            <validValueList displayName="Gender" required="false"
                            autoComplete="true"
                            markedForTask="false">
                <validValue displayName="m: Male" value="m"/>
                <validValue displayName="f: Female" value="f"/>
                <validValue displayName="x: Not Defined" value="x"/>
            </validValueList>

            <cvTermList displayName="Age" required="true"
                        serviceUrl="http://sage.int.janelia.org/sage-ws/cvs/age_remap"
                        displayNamePrefixedForValues="true"
                        autoComplete="true"
                        markedForTask="false">
            </cvTermList>

            <text displayName="Full Age" required="false"
                  markedForTask="false">
            </text>

            <cvTermList displayName="Imager" required="true"
                        markedForTask="false"
                        autoComplete="true"
                        sharedForAllSessionFiles="true"
                        serviceUrl="http://sage.int.janelia.org/sage-ws/cvs/imager">
            </cvTermList>

            <validValueList displayName="Annotator" required="false"
                            autoComplete="true"
                            sharedForAllSessionFiles="true"
                            markedForTask="false">
                <validValue value="Aljoscha Nern"/>
                <validValue value="Tanya Wolff"/>
                <validValue value="Yoshi Aso"/>
            </validValueList>

            <validValueList displayName="Tissue Mounting Protocol" required="true"
                            sharedForAllSessionFiles="true"
                            markedForTask="false">
                <validValue value="DPX Ethanol Mounting"/>
                <validValue value="DPX PBS Mounting"/>
                <validValue value="Glycerol-Based Mounting"/>
            </validValueList>

            <validValueList displayName="Screen State" required="true"
                            markedForTask="false"
                            sharedForAllSessionFiles="true"
                            autoComplete="true">
                <validValue value="pre_screen"/>
                <validValue value="accepted" default="true"/>
                <validValue value="accepted_public"/>
                <validValue value="rejected"/>
            </validValueList>

            <cvTermList displayName="Imaging Project" required="true"
                        autoComplete="true"
                        markedForTask="false"
                        displayNamePrefixedForValues="false"
                        serviceUrl="http://sage.int.janelia.org/sage-ws/cvs/imaging_project">
                <staticDefault value="mcfo"/>
            </cvTermList>

            <text displayName="Channel Specification" required="false"
                  markedForTask="false"
                  pattern="s*rs*">
            </text>

            <text displayName="Channel 1 Name" required="false"
                  markedForTask="false"
                  editable="false">
            </text>

            <decimal displayName="Channel 1 Power" required="false"
                     markedForTask="false"
                     editable="false">
            </decimal>

            <decimal displayName="Channel 1 Gain" required="false"
                     markedForTask="false"
                     editable="false">
            </decimal>

            <text displayName="Channel 2 Name" required="false"
                  markedForTask="false"
                  editable="false">
            </text>

            <decimal displayName="Channel 2 Power" required="false"
                     markedForTask="false"
                     editable="false">
            </decimal>

            <decimal displayName="Channel 2 Gain" required="false"
                     markedForTask="false"
                     editable="false">
            </decimal>

            <text displayName="Channel 3 Name" required="false"
                  markedForTask="false"
                  editable="false">
            </text>

            <decimal displayName="Channel 3 Power" required="false"
                     markedForTask="false"
                     editable="false">
            </decimal>

            <decimal displayName="Channel 3 Gain" required="false"
                     markedForTask="false"
                     editable="false">
            </decimal>

        </dataFields>

    </project>

</transmogrifierConfiguration>