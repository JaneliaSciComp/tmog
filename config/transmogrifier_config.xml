<transmogrifierConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                     xsi:noNamespaceSchemaLocation="transmogrifier_config.xsd">

    <global minimumVersion="2.7.0"/>

    <project name="Fly Light Split (FLSP*)" default="true">

        <inputFileSorter sortAlgorithm="LNumber" />

        <plugins>

        </plugins>

        <dataFields>

            <separator value="FLSP_"/>
            <text displayName="Concentration" required="true"
                  pattern="[0-9A-Za-z_]++" padFormat="40">
                <sourceFileDefault pattern=".+L\d\d??_.*-(\d+).*#.*"/>
            </text>
            <runTime datePattern="yyyyMMddHHmmssSSS" />
            <separator value="_"/>
            <pluginData displayName="image_number"/>
            <separator value=".lsm"/>

         </dataFields>

    </project>

