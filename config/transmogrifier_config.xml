        <!-DASH_FOR_PROD -->

        <plugins>
            <!-- DASH_FOR_PROD->
            <sessionListener className="org.janelia.it.ims.tmog.plugin.SessionSummaryFileWriter">
                <property name="directory" value="\\dm6.int.janelia.org\rubindata\renamer\summaries\RD"/>
            </sessionListener>
