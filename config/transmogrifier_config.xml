<?xml version="1.0"?>

<transmogrifierConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                     xsi:noNamespaceSchemaLocation="transmogrifier_config.xsd">

    <global minimumVersion="2.2.2"/>

    <project name="Test Project" default="true">

        <plugins>
        </plugins>

        <dataFields>
            <separator value="BSB_"/>

            <text displayName="Line" pattern="[0-9A-Za-z\-\_]++" />
            <text displayName="Line2" pattern="[0-9A-Za-z\-\_]++"
                  sharedForAllSessionFiles="true"/>

            <number displayName="Num" minimumValue="0" maximumValue="5"/>
            <number displayName="Num2" minimumValue="0" maximumValue="5"
                    sharedForAllSessionFiles="true"/>

            <validValueList displayName="List" required="true"
                            markedForTask="false">
                <validValue displayName="jenetta" value="jenetta"/>
                <validValue displayName="ngot" value="ngot"/>
                <validValue displayName="trautmane" value="trautmane"/>
            </validValueList>
            <validValueList displayName="List2" required="true"
                            markedForTask="false"
                            sharedForAllSessionFiles="true">
                <validValue displayName="jenetta" value="jenetta"/>
                <validValue displayName="ngot" value="ngot"/>
                <validValue displayName="trautmane" value="trautmane"/>
            </validValueList>
            
            <runTime datePattern="'_'yyyyMMddHHmmssSSS"/>
            <separator value=".lsm"/>
        </dataFields>

    </project>

</transmogrifierConfiguration>