    <project name="Simpson Lab: GAL4 Lines (JHS_*)">

        <inputFileSorter sortAlgorithm="LNumber" />

        <!-- DASH_FOR_PROD->
        <outputDirectory>
            <path path="\\dm8.int.janelia.org\simpsonmicro\GAL4_confocal_stacks\" />
            <renameFieldValue fieldDisplayName="Line" />
            <renameFieldValue fieldDisplayName="Insertion Number" prefix="_" />
            <path path="-GAL4" />
        </outputDirectory>
        <!-DASH_FOR_PROD -->

        <plugins>
        <!-- DASH_FOR_PROD->
            <rowListener className="org.janelia.it.simpsonlab.SimpsonImageManager" />
            <sessionListener className="org.janelia.it.ims.tmog.plugin.SessionSummaryFileWriter">
                <property name="directory" value="\\dm8.int.janelia.org\simpsonmicro\renamer\summaries\RD"/>
            </sessionListener>
        <!-DASH_FOR_PROD -->
        </plugins>

        <dataFields>
            <separator value="JHS_"/>
            <text displayName="Line" required="true" pattern="[0-9A-Za-z]++">
                <sourceFileDefault pattern="([0-9A-Za-z]++)-?.*-Gal4-.*\.lsm"/>
                <sourceFileDefault pattern=".*[/\\\\]([0-9A-Za-z]++)-?.*-Gal4-.*\.lsm"
                                   matchType="path"/>
            </text>
            <text displayName="Insertion Number" required="false"
                  pattern="[0-9A-Za-z\\-]++" prefix="_">
                <sourceFileDefault pattern="[0-9A-Za-z]++-(.*)-Gal4-.*\.lsm"/>
                <sourceFileDefault pattern=".*mdb[/\\\\].*Gal4-(.*)-UAS.*mdb[/\\\\].*\.lsm"
                                   matchType="path"/>
            </text>
            <separator value="-"/>
            <separator name="Family" value="GAL4"/>
            <separator value="_"/>
            <!--<fileModificationTime displayName="Capture Date" datePattern="yyyyMMdd'_'"/>-->
            <date displayName="Capture Date" required="true" 
                  datePattern="yyyyMMdd" suffix="_">
                <sourceFileDateDefault pattern=".*[-/\\\\](\d\d?-\d\d?-\d\d){1}.*\.lsm"
                                       fromDatePattern="MM-dd-yy"
                                       toDatePattern="yyyyMMdd"
                                       matchType="path"/>
            </date>
            <validValueList displayName="Organ" required="true">
                <validValue displayName="B: Brain" value="B"/>
                <validValue displayName="T: Thorax" value="T"/>
            </validValueList>
            <pluginData displayName="Specimen Number" format="%02d"/>
            <runTime datePattern="'_'yyyyMMddHHmmssSSS"/>
            <separator value=".lsm"/>
        </dataFields>

    </project>

    <project name="Kerr Lab: Multi-Worm Tracker" taskName="collector">

        <inputFileFilter excludeQueryUrl="http://localhost:8080/wormTracker/experiment" />
        
        <plugins>
        <!-- DASH_FOR_PROD->
            <rowListener className="org.janelia.it.ims.tmog.plugin.RestDataPlugin">
                <property name="dataServerUrl" value="http://localhost:8080/wormTracker/experiment"/>
                <property name="resourceField" value="Experiment Timestamp"/>
            </rowListener>
        <!-DASH_FOR_PROD -->
        </plugins>

        <dataFields>
            <text displayName="Worm Strain" required="true" pattern="[0-9A-Za-z\s]++"/>
            <text displayName="RNAI" required="true" pattern="[0-9A-Za-z\s]++"/>
            <validValueList displayName="Stimulus Protocol" required="true">
                <validValue displayName="A: Protocol A" value="A"/>
                <validValue displayName="B: Protocol B" value="B"/>
            </validValueList>
            <text displayName="Path" required="true" copyable="false">
                <sourceFileDefault pattern=".*[/\\\\]([^/\\\\]++)[/\\\\]\d\d\d\d\d\d\d\d_\d\d\d\d\d\d"
                                   matchType="path"/>
            </text>
            <date displayName="Experiment Timestamp" required="true"
                  datePattern="yyyyMMdd_HHmmss" copyable="false">
                <sourceFileDateDefault pattern="(\d\d\d\d\d\d\d\d_\d\d\d\d\d\d){1}"
                                       fromDatePattern="yyyyMMdd_HHmmss"
                                       toDatePattern="yyyyMMdd_HHmmss"
                                       matchType="name"/>
            </date>
            <text displayName="Tracker Tag" required="true" pattern="[0-9A-Za-z\s]++"/>
            <text displayName="Notes" required="false" />
        </dataFields>

    </project>

</transmogrifierConfiguration>