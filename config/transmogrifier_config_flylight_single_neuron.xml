<?xml version="1.0"?>

<transmogrifierConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                     xsi:noNamespaceSchemaLocation="transmogrifier_config.xsd">

    <global minimumVersion="3.0.7" frameSizePercentage="95"/>

    <project name="Fly Light Flip - manual entry (FLFL*)" default="true">

        <inputFileFilter patternString=".+\.lsm" />

        <inputFileSorter sortAlgorithm="Number"
                         patternString=".*_(L)(\d++)(\.lsm)"/>

        <outputDirectory basePath="[\\dm11.janelia.priv|/groups/flylight]/flylight/flip/confocalStacks"
                         derivedFromEarliestModifiedFile="true"/>

        <plugins>

            <rowUpdater className="org.janelia.it.ims.tmog.plugin.dataFile.TileToDataSetPlugin">
                <property name="wolfft-01" value="Tanya Wolff:Central Brain,.:wolfft_central_tile,."/>
                <property name="nerna-01" value="Aljoscha Nern:Left Optic Lobe,.:nerna_optic_lobe_left,."/>
                <property name="nerna-02" value="Aljoscha Nern:Right Optic Lobe,.:nerna_optic_lobe_right,."/>
                <property name="nerna-03" value="Aljoscha Nern:Left Optic Lobe,.,Right Optic Lobe,.:nerna_optic_lobe_left,.,nerna_optic_lobe_right,."/>
                <property name="nerna-04" value="Aljoscha Nern:Left Optic Lobe,.,Ventral Brain,.,Left Dorsal Brain,.,Right Dorsal Brain,.,Right Optic Lobe,.:nerna_whole_brain,.,.,.,.,.,.,.,.,."/>
                <property name="nerna-05" value="Aljoscha Nern:Left Optic Lobe,.,Left Central Brain,.,Right Central Brain,.,Right Optic Lobe,.:nerna_optic_span,.,.,.,.,.,.,."/>
                <property name="nerna-06" value="Aljoscha Nern:Left Optic Lobe,.,Central Brain,.,Right Optic Lobe,.:nerna_optic_span,.,.,.,.,."/>
                <property name="nerna-07" value="Aljoscha Nern:Left Optic Lobe,.,Left Central Brain,.,Right Central Brain,.:nerna_optic_central_border,.,.,.,.,."/>
                <property name="nerna-08" value="Aljoscha Nern:Left Central Brain,.,Right Central Brain,.,Right Optic Lobe,.:nerna_optic_central_border,.,.,.,.,."/>
                <property name="nerna-09" value="Aljoscha Nern:Left Dorsal Brain,.,Right Dorsal Brain,.,Right Optic Lobe,.:nerna_optic_central_border,.,.,.,.,."/>
                <property name="nerna-10" value="Aljoscha Nern:Left Optic Lobe,.,Ventral Brain,.,Left Dorsal Brain,.:nerna_optic_central_border,.,.,.,.,."/>
                <property name="nerna-11" value="Aljoscha Nern:Left Optic Lobe,.,Left Central Brain,.,Right Optic Lobe,.:nerna_optic_central_border,.,.,.,nerna_optic_lobe_right,."/>
                <property name="nerna-12" value="Aljoscha Nern:Left Optic Lobe,.,Right Central Brain,.,Right Optic Lobe,.:nerna_optic_lobe_left,.,nerna_optic_central_border,.,.,."/>
                <property name="nerna-13" value="Aljoscha Nern:Left Optic Lobe,.,Left Central Brain,.:nerna_optic_central_border,.,.,."/>
                <property name="nerna-14" value="Aljoscha Nern:Right Central Brain,.,Right Optic Lobe,.:nerna_optic_central_border,.,.,."/>
            </rowUpdater>

            <rowValidator className="org.janelia.it.ims.tmog.plugin.CrossFieldValidator">
                <property name="validateField" value="Gender"/>
                <property name="matchesPattern" value="\S+"/>
                <property name="whenReferenceField" value="Age"/>
                <property name="matchesReferencePattern" value="^[^L].*"/>
                <property name="errorMessage" value="Gender value is required when age is ${Age}."/>
            </rowValidator>

            <rowListener className="org.janelia.it.ims.tmog.plugin.imagedb.ImageSequencePlugin">
                <property name="db.config.key" value="sage" />
                <property name="image_number" value="urn:lsid:janelia.org:flylight_flip:image_number" />
            </rowListener>

            <rowListener className="org.janelia.it.ims.tmog.plugin.imagedb.ImageDataPlugin">
                <property name="db.config.key" value="sage"/>
                <property name="lab" value="$STATIC:flylight"/>
                <property name="family" value="$STATIC:flylight_flip"/>
                <property name="imaging_project" value="Imaging Project"/>
                <property name="line" value="GAL4 Line"/>
                <property name="created_by" value="Imager"/>
                <property name="effector" value="Effector"/>
                <property name="slide_code" value="Slide Code"/>
                <property name="tile" value="Tile"/>
                <property name="gender" value="Gender"/>
                <property name="age" value="Age"/>
                <property name="full_age" value="Full Age"/>
                <property name="area" value="$STATIC:Brain"/>
                <property name="heat_shock_minutes" value="Heat Shock Minutes"/>
                <property name="annotated_by" value="Annotator"/>
                <property name="data_set" value="Data Set"/>
                <property name="microscope_filename"/>
            </rowListener>

            <!--
                  The companion file listener (for log files) should be the
                  last defined plug-in to ensure that log files are only
                  copied if the source lsm processing succeeds.
            -->
            <rowListener className="org.janelia.it.ims.tmog.plugin.CompanionFileRowListener" >
                <property name="sourceSuffix" value=".lsm"/>
                <property name="companionSuffix" value="_log.csv"/>
                <property name="deleteAfterCopy" value="true"/>
            </rowListener>

        </plugins>

        <dataFields>

            <separator value="FLFL_"/>
            <runTime datePattern="yyyyMMddHHmmssSSS" />
            <separator value="_"/>
            <pluginData displayName="image_number"/>
            <separator value=".lsm"/>

            <!--
             These values are collected for storage but are not
             included as components of the file name (markedForTask="false").
             -->

            <webServiceList displayName="GAL4 Line" required="true"
                            markedForTask="false"
                            valueCreationPath="*/line"
                            relativeActualValuePath="name"
                            relativeValueDisplayNamePath="name"
                            serviceUrl="http://sage.int.janelia.org/sage-ws/lines/forMultipleLabs.janelia-sage?lab=fly&amp;lab=flylight&amp;lab=rubin&amp;lab=jayaraman&amp;lab=simpson&amp;lab=leet&amp;lab=zlatic"
                            autoComplete="true">
            </webServiceList>

            <cvTermList displayName="Effector" required="true"
                        autoComplete="true"
                        markedForTask="false"
                        displayNamePrefixedForValues="false"
                        serviceUrl="http://sage.int.janelia.org/sage-ws/cvs/effector">
            </cvTermList>

            <text displayName="Slide Code" required="true"
                  pattern="\d{8}_\d+_[A-Z]\d+"
                  copyable="false"
                  markedForTask="false">
                <sourceFileDefault pattern="^([A-Z]\d+)_ZB.*_(\d{8}_\d+_).*"
                                   patternGroupSpec="2,1"/>
            </text>

            <validValueList displayName="Tile" required="true"
                            autoComplete="true"
                            markedForTask="false">
                <sourceFileMappedDefault pattern="^.*_T\d+_([A-Z]+)_\d{8}_\d+_.*">
                    <mappedValue from="CEN" to="Central Brain"/>
                    <mappedValue from="L" to="Left Optic Lobe"/>
                    <mappedValue from="LC" to="Left Central Brain"/>
                    <mappedValue from="LD" to="Left Dorsal Brain"/>
                    <mappedValue from="R" to="Right Optic Lobe"/>
                    <mappedValue from="RC" to="Right Central Brain"/>
                    <mappedValue from="RD" to="Right Dorsal Brain"/>
                    <mappedValue from="V" to="Ventral Brain"/>
                </sourceFileMappedDefault>
                <validValue displayName="01: Left Optic Lobe" value="Left Optic Lobe" />
                <validValue displayName="02: Ventral Brain" value="Ventral Brain"/>
                <validValue displayName="03: Left Dorsal Brain" value="Left Dorsal Brain"/>
                <validValue displayName="04: Right Dorsal Brain" value="Right Dorsal Brain"/>
                <validValue displayName="05: Right Optic Lobe" value="Right Optic Lobe"/>
                <validValue displayName="06: Central Brain" value="Central Brain"/>
                <validValue displayName="07: Left Central Brain" value="Left Central Brain"/>
                <validValue displayName="08: Right Central Brain" value="Right Central Brain"/>
            </validValueList>

            <validValueList displayName="Gender" required="false"
                            autoComplete="true"
                            markedForTask="false">
                <validValue displayName="m: Male" value="m"/>
                <validValue displayName="f: Female" value="f"/>
                <validValue displayName="x: Not Defined" value="x"/>
            </validValueList>

            <cvTermList displayName="Age" required="true"
                        serviceUrl="http://sage.int.janelia.org/sage-ws/cvs/age_remap"
                        displayNamePrefixedForValues="true"
                        autoComplete="true"
                        markedForTask="false">
            </cvTermList>

            <text displayName="Full Age" required="false"
                  markedForTask="false">
            </text>

            <decimal displayName="Heat Shock Minutes" required="false"
                     minimumValue="0" markedForTask="false" />

            <cvTermList displayName="Imager" required="true"
                        markedForTask="false"
                        autoComplete="true"
                        serviceUrl="http://sage.int.janelia.org/sage-ws/cvs/imager">
            </cvTermList>

            <validValueList displayName="Annotator" required="false"
                            autoComplete="true"
                            markedForTask="false">
                <validValue value="Aljoscha Nern"/>
                <validValue value="Yoshi Aso"/>
                <validValue value="Tanya Wolff"/>
            </validValueList>

            <webServiceList displayName="Data Set" required="false"
                            markedForTask="false"
                            valueCreationPath="*/dataSet"
                            relativeActualValuePath="dataSetIdentifier"
                            relativeValueDisplayNamePath="dataSetIdentifier"
                            serviceUrl="http://jacs.int.janelia.org:8180/rest-v1/dataSet?user=group:flylight&amp;user=user:asoy&amp;user=user:wolfft&amp;user=user:nerna"
                            autoComplete="true">
            </webServiceList>

            <cvTermList displayName="Imaging Project" required="true"
                        autoComplete="true"
                        markedForTask="false"
                        displayNamePrefixedForValues="false"
                        serviceUrl="http://sage.int.janelia.org/sage-ws/cvs/imaging_project">
                <staticDefault value="mcfo"/>
            </cvTermList>

        </dataFields>

    </project>

    <project name="Fly Light Flip - preScreen - scope to dm11">

        <inputFileFilter patternString=".+\.lsm" />

        <!-- NOTES:
             - first capturing group must be equal for second capturing group
               to be used in numeric sort
             - third capturing group only used when first and second are equal
             - '(?: )' identifies a non-capturing group
        -->
        <inputFileSorter sortAlgorithm="Number"
                         patternString="(?:^|.*_)(B)(\d+)(_.*)"/>

        <outputDirectory>
            <path path="[\\dm11.janelia.priv|/groups/flylight]/flylight/flip/preScreen/" />
            <renameFieldValue fieldDisplayName="Mount Date"/>
            <path path="_"/>
            <renameFieldValue fieldDisplayName="Slide Number"/>
            <path path="/"/>
            <renameFieldValue fieldDisplayName="Magnification"/>
        </outputDirectory>

        <plugins>
            <rowUpdater className="org.janelia.it.ims.tmog.plugin.dataFile.DataResourcePlugin">
                <property name="plugin.service-url" value="http://sage.int.janelia.org/flyboy-ws/project_crosses/withSerialNumber/${Cross Serial Number}" />
                <property name="plugin.test-url" value="http://sage.int.janelia.org/flyboy-ws/project_crosses/withSerialNumber/51418" />
                <property name="plugin.root-xpath" value="projectCross" />
                <property name="Line" value="parent1StockName" />
                <property name="Reporter" value="reporter" />
            </rowUpdater>

            <!--
                  The companion file listener (for log files) should be the
                  last defined plug-in to ensure that log files are only
                  copied if the source lsm processing succeeds.
            -->
            <rowListener className="org.janelia.it.ims.tmog.plugin.CompanionFileRowListener" >
                <property name="sourceSuffix" value=".lsm"/>
                <property name="companionSuffix" value="_log.csv"/>
                <property name="deleteAfterCopy" value="true"/>
            </rowListener>

        </plugins>

        <dataFields>

            <text displayName="Line" required="true"
                  suffix="__"/>

            <number displayName="Cross Serial Number" required="true"
                  minimumValue="1"
                  maximumValue="99999"
                  suffix="_">
            </number>

            <date displayName="Mount Date" required="true"
                  datePattern="yyyyMMdd"
                  suffix="_">
                <sourceFileDateDefault pattern=".*_(\d{8})_.*"
                                       fromDatePattern="yyyyMMdd"
                                       toDatePattern="yyyyMMdd"/>
            </date>

            <text displayName="Slide Number" required="true"
                  pattern="[A-Za-z]*\d+"
                  suffix="_">
                <sourceFileDefault pattern=".*_\d{8}_([^_]+)_.*"/>
            </text>

            <text displayName="Slide Location" required="true"
                  pattern="[A-Z]\d\d?"
                  convertToUpperCase="true"
                  suffix="_">
                <sourceFileDefault pattern="^([^_]+)_ZB.*"/>
                <sourceFileSlideLocationDefault pattern="(?:^|.*_)B(\d+)_.*"
                                                numberOfSlideColumns="6"/>
            </text>

            <number displayName="Zeiss Brain" required="true"
                    minimumValue="1"
                    maximumValue="99"
                    padFormat="20"
                    copyable="false"
                    prefix="zb"
                    suffix="_">
                <sourceFileDefault pattern=".*_[Zz][Bb](\d+)_.*"/>
                <sourceFileDefault pattern="(?:^|.*_)B(\d+)_.*"/>
            </number>

            <number displayName="Zeiss Tile" required="true"
                    minimumValue="1"
                    maximumValue="99"
                    padFormat="20"
                    prefix="zt"
                    suffix="_">
                <sourceFileDefault pattern=".*_zt(\d+)_.*"/>
                <sourceFileDefault pattern=".*_T(\d+)_.*"/>
            </number>

            <validValueList displayName="Magnification" required="true"
                            autoComplete="true"
                            suffix="_">
                <sourceFileDefault pattern=".*_(\d\dX)_.*"/>
                <sourceFileMappedDefault pattern=".*_(\d\dx)_.*">
                    <mappedValue from="20x" to="20X" />
                    <mappedValue from="63x" to="63X" />
                </sourceFileMappedDefault>
                <validValue value="20X"/>
                <validValue value="63X"/>
            </validValueList>

            <!-- channel information stub -->
            <separator value="0-0-0_"/>

            <text displayName="Reporter"
                  prefix="_"
                  suffix="__">
            </text>

            <text displayName="Suffix" required="true"
                  pattern="R\d+_L\d+"
                  copyable="false">
                <sourceFileDefault pattern=".*_(R\d+_L\d+).*"/>
            </text>

            <separator value="_"/>
            <runTime datePattern="yyyyMMddHHmmssSSS" />
            <separator value=".lsm"/>

        </dataFields>

    </project>

