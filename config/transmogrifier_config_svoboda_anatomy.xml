<?xml version="1.0"?>

<transmogrifierConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                     xsi:noNamespaceSchemaLocation="transmogrifier_config.xsd">

    <!-- frameSizePercentage default is 80, min is 40, and max is 99 --> 
    <global minimumVersion="2.3.0"
            frameSizePercentage="95"/>

    <project name="Svoboda Lab: Anatomy Project" default="true">

        <inputFileFilter patternString=".+\.(tiff|jpg|lsm)"
                         recursiveSearch="true"/>

        <plugins>
            <rowListener className="org.janelia.it.ims.tmog.plugin.XmlWriterPlugin" >
                <property name="directory" value="\\dm6.int.janelia.org\svobodalab\tmog\xml-anatomy"/>
            </rowListener>
        </plugins>

        <dataFields>

            <separator value="KS-A_"/>

            <validValueList displayName="Project" required="true">
                <validValue displayName="Project A" value="A"/>
                <validValue displayName="Project B" value="B"/>
            </validValueList>

            <fileName displayName="Source File Format"
                      patternString=".+\.(.+)"
                      patternGroupNumber="1"
                      visible="true"
                      markedForTask="false"/>

            <fileName displayName="File Base Name"
                      patternString="(.+)\..+"
                      patternGroupNumber="1"
                      visible="true" />

            <number displayName="First File Number"
                    minimumValue="0"
                    markedForTask="false"/>

            <number displayName="Last File Number"
                    minimumValue="0"
                    markedForTask="false"/>

            <validValueList displayName="Compression" required="true">
                <validValue value="NA"/>
            </validValueList>

            <validValueList displayName="Imager"
                            required="true"
                            markedForTask="false">
                <validValue displayName="Andrew Hires" value="hiresa"/>
                <validValue displayName="Daniel Huber" value="huberd"/>
                <validValue displayName="Dan O'Connor" value="oconnord"/>
                <validValue displayName="Karel Svoboda" value="svobodak"/>
                <validValue displayName="Leopoldo Petreanu" value="petreanul"/>
                <validValue displayName="Mac Hooks" value="hooksm"/>
                <validValue displayName="Simon Peron" value="perons"/>
                <validValue displayName="Takaki Komiyama" value="komiyamat"/>
                <validValue displayName="Takashi Sato" value="satot"/>
                <validValue displayName="Tianyi Mao" value="maot"/>
            </validValueList>

            <validValueList displayName="Microscope"
                            required="true"
                            markedForTask="false">
                <validValue value="nanozoomer_olympus"/>
                <validValue value="axioscope_zeiss"/>
                <validValue value="macroscope_olympus"/>
                <validValue value="confocal710_zeiss"/>
            </validValueList>

            <text displayName="Animal ID" required="true"/>

            <validValueList displayName="Strain" required="true">
                <validValue value="c57b6"/>
                <validValue value="sv129"/>
                <validValue value="fvb"/>
            </validValueList>

            <validValueList displayName="Transgene 1" required="true">
                <validValue value="thy-1_Chr2GFP_line9_feng"/>
                <validValue value="Ctgf_EGFP_gensat"/>
                <validValue value="ETV1_cre_GM225_gensat"/>
            </validValueList>
            <validValueList displayName="Transgene 2" required="true">
                <validValue value="thy-1_Chr2GFP_line9_feng"/>
                <validValue value="Ctgf_EGFP_gensat"/>
                <validValue value="ETV1_cre_GM225_gensat"/>
            </validValueList>
            <validValueList displayName="Transgene 3" required="true">
                <validValue value="thy-1_Chr2GFP_line9_feng"/>
                <validValue value="Ctgf_EGFP_gensat"/>
                <validValue value="ETV1_cre_GM225_gensat"/>
            </validValueList>

            <number displayName="Perfusion Age" required="true" minimumValue="0" maximumValue="600"/>

            <date displayName="Perfusion Date" required="true" datePattern="MM/dd/yyyy"/>

            <validValueList displayName="Perfusion Who" required="true">
                <validValue displayName="Andrew Hires" value="hiresa"/>
                <validValue displayName="Daniel Huber" value="huberd"/>
                <validValue displayName="Dan O'Connor" value="oconnord"/>
                <validValue displayName="Karel Svoboda" value="svobodak"/>
                <validValue displayName="Leopoldo Petreanu" value="petreanul"/>
                <validValue displayName="Mac Hooks" value="hooksm"/>
                <validValue displayName="Simon Peron" value="perons"/>
                <validValue displayName="Takaki Komiyama" value="komiyamat"/>
                <validValue displayName="Takashi Sato" value="satot"/>
                <validValue displayName="Tianyi Mao" value="maot"/>
            </validValueList>

            <validValueList displayName="Cutting Who" required="true">
                <validValue displayName="Andrew Hires" value="hiresa"/>
                <validValue displayName="Daniel Huber" value="huberd"/>
                <validValue displayName="Dan O'Connor" value="oconnord"/>
                <validValue displayName="Karel Svoboda" value="svobodak"/>
                <validValue displayName="Leopoldo Petreanu" value="petreanul"/>
                <validValue displayName="Mac Hooks" value="hooksm"/>
                <validValue displayName="Simon Peron" value="perons"/>
                <validValue displayName="Takaki Komiyama" value="komiyamat"/>
                <validValue displayName="Takashi Sato" value="satot"/>
                <validValue displayName="Tianyi Mao" value="maot"/>
            </validValueList>

            <number displayName="Section Thickness" required="true" minimumValue="0" maximumValue="10000"/>

            <validValueList displayName="Section Orientation" required="true">
                <validValue value="coronal"/>
                <validValue value="saggital"/>
            </validValueList>

            <number displayName="Section Range Start" required="true"/>
            <number displayName="Section Range End" required="true"/>
            <number displayName="Number of Sections" required="true" minimumValue="0" maximumValue="10000"/>

            <fieldGroup displayName="Labels">

                <validValueList displayName="Injector"
                                required="true"
                                markedForTask="false">
                    <validValue displayName="Andrew Hires" value="hiresa"/>
                    <validValue displayName="Daniel Huber" value="huberd"/>
                    <validValue displayName="Dan O'Connor" value="oconnord"/>
                    <validValue displayName="Karel Svoboda" value="svobodak"/>
                    <validValue displayName="Leopoldo Petreanu" value="petreanul"/>
                    <validValue displayName="Mac Hooks" value="hooksm"/>
                    <validValue displayName="Simon Peron" value="perons"/>
                    <validValue displayName="Takaki Komiyama" value="komiyamat"/>
                    <validValue displayName="Takashi Sato" value="satot"/>
                    <validValue displayName="Tianyi Mao" value="maot"/>
                </validValueList>

                <validValueList displayName="Label"
                                required="true"
                                markedForTask="false">
                    <validValue value="AAV_21_CAG-ChR2Venus_ks"/>
                    <validValue value="AAV_21_CAG-flexChR2mCherry_ss"/>
                    <validValue value="AAV_29_CMV_dsRedEx_JH"/>
                    <validValue value="AAV_21_syn_GCaM3.3_LL"/>
                    <validValue value="AAV_21_Flex_CAG_GFP_ks"/>
                    <validValue value="AAV_21_CAG_eGFP_ks"/>
                </validValueList>

                <number displayName="Injection Age"
                        required="true"
                        minimumValue="0"
                        maximumValue="600"
                        markedForTask="false"/>

                <number displayName="Injection Volume"
                        required="true"
                        minimumValue="0"
                        maximumValue="10000"
                        markedForTask="false"/>

                <number displayName="Dilution"
                        required="true"
                        minimumValue="0"
                        markedForTask="false"/>

                <validValueList displayName="Target Brain Region"
                                required="true">
                    <validValue value="vM1"/>
                    <validValue value="S1_bc"/>
                    <validValue value="PO"/>
                    <validValue value="VPM"/>
                    <validValue value="ZI"/>
                </validValueList>

                <number displayName="Bregma ap"
                        required="true"
                        minimumValue="0"
                        markedForTask="false"/>

                <number displayName="Bregma ml"
                        required="true"
                        minimumValue="0"
                        markedForTask="false"/>

                <number displayName="Bregma depth"
                        required="true"
                        minimumValue="0"
                        markedForTask="false"/>

            </fieldGroup>

            <fieldGroup displayName="Channel">

                <validValueList displayName="Fluorophore"
                                required="true"
                                markedForTask="false">
                    <validValue value="GFP"/>
                    <validValue value="tdTomato"/>
                    <validValue value="mCherry"/>
                    <validValue value="Alexa-594"/>
                    <validValue value="Darkfield_450nm"/>
                </validValueList>

                <validValueList displayName="Antigen"
                                required="true"
                                markedForTask="false">
                    <validValue value="GFP"/>
                </validValueList>

                <number displayName="Number of Channel Sections"
                        required="true"
                        minimumValue="0"
                        markedForTask="false"/>

                <number displayName="Channel Section Spacing"
                        required="true"
                        minimumValue="1"
                        maximumValue="10000"
                        markedForTask="false"/>

                <number displayName="Channel Section Range Start"
                        required="true"
                        markedForTask="false"/>

                <number displayName="Channel Section Range End"
                        required="true"
                        markedForTask="false"/>
                                       
            </fieldGroup>

            <runTime datePattern="_yyyyMMddHHmmssSSS"/>
            <fileExtension/>

        </dataFields>

    </project>

</transmogrifierConfiguration>